#include<iostream>
#include<cmath>
using namespace std;

int main() {
    int a,b,c;
    int D,x1,x2;
    cin>>a>>b>>c;

    D=(b*b)-(4*a*c);
    x1 = (-b + sqrt(D) )/(2*a);
    x2 = (-b - sqrt(D) )/(2*a);
    
    if(D==0){
        cout<<"real and equal"<<endl;
        cout<<x1<<" "<<x2;
    }

    else if(D<0){
        cout<<"imaginary"<<endl;
    }

    else {
        cout<<"real and distinct"<<endl;
        cout<<x1<<" "<<x2;
    }
    
return 0;
}
