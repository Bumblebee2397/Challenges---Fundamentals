#include<iostream>
#include<cmath>
using namespace std;


int main() {

    int bin,exp=0,sum=0,val=0;

        cin>>bin;
        
        while(bin>0){
            val=(bin%10)*pow(2,exp);
            bin=bin/10;
            exp++;
            sum=sum+val;
        }

    cout<<sum;
    return 0;
}
